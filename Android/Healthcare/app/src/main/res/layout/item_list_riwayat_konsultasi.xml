<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="32dp"
    android:layout_marginRight="32dp"
    android:layout_marginTop="20dp"
    android:background="@drawable/myshape">

    <TextView
        android:id="@+id/id_riwayat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        android:text="Id Riwayat "
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/id_pasien"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toBottomOf="@id/id_riwayat"
        android:text="Id Pasien "
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/id_dokter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toBottomOf="@id/id_pasien"
        android:text="Id Dokter "
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/riwayat_penyakit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toBottomOf="@id/id_dokter"
        android:text="Riwayat penyakit " />

    <TextView
        android:id="@+id/detail_riwayat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toBottomOf="@id/riwayat_penyakit"
        android:text="Detail riwayat" />

    <TextView
        android:id="@+id/waktu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toBottomOf="@id/detail_riwayat"
        android:text="Waktu " />

</androidx.constraintlayout.widget.ConstraintLayout>